{
  "builds": [
      {
          "src": "api/index.js",  
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
           "src": "/(.*)",
          "dest": "api/index.js",

          "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
           "headers": {
           "Access-Control-Allow-Origin": ["http://localhost:3000", "https://www.wuli.rocks"]
      }  
      } 
  ]
}